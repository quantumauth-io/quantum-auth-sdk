{
  "name": "@quantumauth/node",
  "version": "0.3.0",
  "description": "QuantumAuth backend/middleware SDK",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quantumauth-io/quantum-auth-sdk.git",
    "directory": "packages/web"
  },
  "homepage": "https://github.com/quantumauth-io/quantum-auth-sdk#readme",
  "bugs": {
    "url": "https://github.com/quantumauth-io/quantum-auth-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "tsup": "^8.0.0",
    "express": "^4.21.0",
    "@types/express": "^4.17.21"
  }
}
