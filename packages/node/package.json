{
  "name": "@quantumauth/node",
  "version": "0.2.0",
  "description": "QuantumAuth backend/middleware SDK",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "tsup": "^8.0.0"
  }
}
