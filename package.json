{
  "name": "quantum-auth-sdk",
  "private": true,
  "version": "0.0.0",
  "description": "QuantumAuth SDK Monorepo",
  "license": "Apache-2.0",
  "scripts": {
    "build": "pnpm -F \"@quantumauth/*\" build",
    "build:web": "pnpm -F @quantumauth/web build",
    "build:node": "pnpm -F @quantumauth/node build",
    "dev:next-demo": "pnpm -F quantum-auth-next-demo dev",
    "dev:express-demo": "pnpm -F quantum-auth-express-demo dev",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  }
}
